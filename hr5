import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) 
    {
        Scanner sc=new Scanner(System.in);
        int odd_count=0;
        int start=sc.nextInt();
        int end=sc.nextInt();
        for(int i=start;i<=end;i++)
        {
            if(i%2!=0)
            {
                odd_count+=1;
            }
        }
        System.out.println(odd_count);
    }
    
}
