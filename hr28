import java.util.*;
public class Solution
{
    public static boolean binarySearch(int a,int c)
    {
        int left=0;
        int right=(int)Math.sqrt(c);
        while(left<=right)
        {
            int mid=left+(right-left)/2;
            long sum=(long)a*a+(long)mid*mid;
            if(sum==c)
            {
                return true;
            }
            if(sum<c)
            {
                left=mid+1;
            }
            else
            {
                right=mid-1;
            }
        }
        return false;
    }
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int c=sc.nextInt();
        boolean found=false;
        for(int a=0;a*a<=c;a++)
        {
            if(binarySearch(a,c))
            {
                found=true;
                break;
            }
        }
        if(found)
        {
            System.out.println("true");
        }
        else
        {
            System.out.println("false");
        }
    }
}
